<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="trustpet.Hora" table="Hora" lazy="false">
		<id name="id" column="Id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="hora" column="Hora" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="disponivel" column="Disponivel" type="boolean" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
