<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="trustpet.Dono" table="Dono" lazy="false">
		<id name="id" column="Id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="ativo" column="Ativo" type="boolean" not-null="true" lazy="false"/>
		<property name="nome" column="Nome" type="string" length="255" not-null="false" lazy="false"/>
		<property name="photo" column="Photo" type="string" length="255" not-null="false" lazy="false"/>
		<property name="email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<property name="contacto" column="Contacto" type="string" length="255" not-null="false" lazy="false"/>
		<property name="residencia" column="Residencia" type="string" length="255" not-null="false" lazy="false"/>
		<property name="jardim" column="Jardim" type="boolean" not-null="false" lazy="false"/>
		<property name="morada" column="Morada" type="string" length="255" not-null="false" lazy="false"/>
		<property name="password" column="Password" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Animais" lazy="true" cascade="all-delete-orphan" inverse="false">
			<key column="DonoId" not-null="true"/>
			<one-to-many class="trustpet.Animal"/>
		</set>
	</class>
</hibernate-mapping>
