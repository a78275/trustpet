<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="main.Dia" table="Dia" lazy="false">
		<id name="id" column="Id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="dia" column="Dia" type="integer" length="10" not-null="true" lazy="false"/>
		<set name="ORM_Horas" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="DiaId" not-null="true"/>
			<one-to-many class="main.Hora"/>
		</set>
	</class>
</hibernate-mapping>
