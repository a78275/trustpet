<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="trustpet.PrecoPetsitterServico" table="PrecoPetsitterServico" lazy="false">
		<id name="id" column="Id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="servico" cascade="lock" column="ServicoId" class="trustpet.Servico" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="petsitter" cascade="lock" column="PetsitterUtilizadorEmail" class="trustpet.Petsitter" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="preco" column="Preco" type="float" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
